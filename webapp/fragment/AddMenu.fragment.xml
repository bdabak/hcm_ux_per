<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:core="sap.ui.core">
	<Menu itemSelected=".onAddMenuSelected" items="{designView>/AddMenuOptions}"
	closed=".onAddMenuClosed">
        <items>
            <MenuItem key="{designView>Type}" text="{designView>Description}"/>
        </items>
	</Menu>
</core:FragmentDefinition>